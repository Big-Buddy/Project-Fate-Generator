html
	head
		title User Profile
		link(rel='stylesheet', href='/vendor/bootstrap.min.css')
		script(type='text/javascript', src='/vendor/jquery.min.js')
		script(type='text/javascript', src='/vendor/jquery.form.min.js')
		script(type='text/javascript', src='/vendor/bootstrap.min.js')

	body
		block content
			nav.navbar.navbar-default
				div.container
					div.navbar-header
						a(href='/user_prof')
							img.navbar-brand(src='/img/logo.png', alt='Project Fate Generator')
					div.collapse.navbar-collapse
						ul.nav.navbar-nav.navbar-right
							li
								button#logout.navbar-btn.btn-primary(type='button') Log Out
			div.body-container
				div.container
					div.row
						div.col-md-12
							h2 Behold, your sequence!
					div.column
						ul.list-group
							each item in sequenceData
								div.col-md-6.col-sm-12
									ul.list-group
										if item[1].semester=="Winter"
											li.list-group-item-info
												label=item[1].semester+" semester #"+item[0].id+":"
													each course in item[2].courses
														li.list-group-item-danger(name= "listItem", id="elective")
															label=course.course_program+course.course_number+" "+course.course_name+"\n"
										else
											li.list-group-item-warning
												label=item[1].semester+" semester #"+item[0].id+":"
													each course in item[2].courses
														li.list-group-item-danger(name= "listItem", id="elective")
															label=course.course_program+course.course_number+" "+course.course_name+"\n"
						<!--Data goes here!-->
						<!--Every two semester GENERATE div.clearfix.visible-md-block-->
						<!--For each semester GENERATE div.col-md-6.col-sm-12 + ul.list-group-->
						<!--For each course in semester GENERATE li.list-group-item-->

